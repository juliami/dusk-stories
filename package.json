{
  "name": "dusk-stories",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "next test --watch",
    "typecheck": "tsc --pretty --noEmit",
    "contentful-space-export": "cross-env $(cat .env | xargs) contentful space export --config ./contentful.config.json",
    "generate-types": "npm run contentful-space-export && cf-content-types-generator ./contentful/exports/contentTypes.json -X -o contentful/types",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "tsc",
      "eslint --fix src/"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^17.1.0",
    "@contentful/rich-text-react-renderer": "^16.1.0",
    "add": "^2.0.6",
    "contentful": "^11.7.7",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.1.2",
    "cf-content-types-generator": "^2.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
